{
  "pluginAlias": "homebridge-tivo-channels",
  "pluginType": "accessory",
  "schema": {
      "type": "object",
      "properties": {
          "name": {
            "title": "Friendly Name",
            "type": "string"
            "default": "Channel Switch"
          },
          "ip": {
            "title": "IP Address",
            "type": "string"
          },
          "channel": {
            "title": "Channel",
            "type": "integer",
            "default": 1,
            "minimum": 1
          },
          "verbose": {
            "title": "Verbose logging?",
            "type": "boolean",
            "default": false
          }
    }
  }
}

